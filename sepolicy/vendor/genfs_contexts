# Extcon
genfscon sysfs /devices/platform/soc/200f000.qcom,spmi/spmi-0/spmi0-02/200f000.qcom,spmi:qcom,pmi8940@2:qcom,qpnp-smbcharger/extcon u:object_r:sysfs_extcon:s0
genfscon sysfs /devices/platform/soc/soc:qcom,mdss_wb_panel/extcon u:object_r:sysfs_extcon:s0

# Touchscreen
genfscon proc /touchpanel	u:object_r:proc_touchpanel:s0
genfscon sysfs /touchpanel	u:object_r:proc_touchpanel:s0

# Vibrator
genfscon sysfs /devices/platform/soc/200f000.qcom,spmi/spmi-0/spmi0-03/200f000.qcom,spmi:qcom,pmi8940@3:qcom,haptics@c000/leds/vibrator	u:object_r:sysfs_leds:s0
